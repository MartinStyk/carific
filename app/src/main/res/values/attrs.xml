<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StatisticsDetailItem">
        <attr name="name" format="string" localization="suggested" />
        <attr name="value" format="string" localization="suggested" />
        <attr name="unit" format="string" localization="suggested" />
    </declare-styleable>
    <declare-styleable name="statisticsItemDetail">
        <attr name="statisticsItemDetail" format="reference" />
    </declare-styleable>
    <declare-styleable name="CarificRecyclerView">
        <attr name="sectionBackgroundColor" format="color" />
        <attr name="sectionTitleTextColor" format="color" />
        <attr name="sectionSubTitleTextColor" format="color" />

        <attr name="timeLineCircleColor" format="color" />
        <attr name="timeLineCircleStrokeColor" format="color" />

        <attr name="sectionTitleTextSize" format="dimension" />
        <attr name="sectionSubTitleTextSize" format="dimension" />
        <attr name="isSticky" format="boolean" />
    </declare-styleable>
</resources>